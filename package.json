{
  "name": "ip2country-vue",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run --environment jsdom",
    "test:watch": "vitest --environment jsdom",
    "test:coverage": "vitest run --coverage --environment jsdom",
    "format": "prettier --write \"src/**/*.{js,ts,vue}\" \"tests/**/*.{js,ts}\" \"*.{js,ts,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,vue}\" \"tests/**/*.{js,ts}\" \"*.{js,ts,json,md}\""
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "is-ip": "^5.0.1",
    "uuid": "^13.0.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/vue": "^8.1.0",
    "@types/is-ip": "^3.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "jsdom": "^26.1.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^3.2.4"
  }
